variables("Na_comp", 
       Na_o=ScalarParam(138.0),
       VFonRT=ScalarParam(-1.31045962116),
       K_o=ScalarParam(4.0),
       E_Na=ScalarParam(70.100138335),
       V=ScalarParam(-35),
       Ca_o=ScalarParam(2.0),
       Ca_i=ScalarParam(8.464e-05),
       Na_i=ScalarParam(10.0),)

states("Na current I_Na", 
       h=ScalarParam(0.99869),
       j=ScalarParam(0.99887),
       m=ScalarParam(0.00024676),)

parameters("Na current I_Na", 
       G_NaMax=ScalarParam(12.8),)

parameters("NCX Current I_NaCa", 
       K_mCa=ScalarParam(1.38),
       K_mNa=ScalarParam(87.5),
       eta=ScalarParam(0.35),
       k_NaCa=ScalarParam(0.3),
       k_sat=ScalarParam(0.2),)

parameters("Na-K pump current I_NaK", 
       I_NaKMax=ScalarParam(0.693),
       K_mKo=ScalarParam(1.5),
       K_mNai=ScalarParam(10.0),)

parameters("Na background current I_bNa", 
       G_bNaMax=ScalarParam(0.0031),)


component("Na current I_Na")
I_Na = G_NaMax*h*j*m**3.0*(-E_Na + V)
a_h = Conditional(Ge(V, -40.0), 0.0, 1.04951082542696e-6*exp(-0.147058823529412*V))
b_h = Conditional(Ge(V, -40.0), 1.0/(0.0497581410839387*exp(-0.0900900900900901*V) + 0.13), 3.56*exp(0.079*V) + 310000.0*exp(0.35*V))
a_j = Conditional(Ge(V, -40.0), 0.0, (V + 37.78)*(-3.474e-5*exp(-0.04391*V) - 127140.0*exp(0.2444*V))/(50262745825.954*exp(0.311*V) + 1.0))
b_j = Conditional(Ge(V, -40.0), 0.3*exp(-2.535e-7*V)/(0.0407622039783662*exp(-0.1*V) + 1.0), 0.1212*exp(-0.01052*V)/(0.00396086833990426*exp(-0.1378*V) + 1.0))
a_m = (0.32*V + 15.0816)/(-0.00897780373069724*exp(-0.1*V) + 1.0)
b_m = 0.08*exp(-V/11.0)

component("NCX Current I_NaCa")
I_NaCa = 5000.0*k_NaCa*(-Ca_i*Na_o**3.0*exp(VFonRT*(eta - 1.0)) + Ca_o*Na_i**3.0*exp(VFonRT*eta))/((Ca_o + K_mCa)*(K_mNa**3.0 + Na_o**3.0)*(k_sat*exp(VFonRT*(eta - 1.0)) + 1.0))

component("Na-K pump current I_NaK")
sigma = 0.142857142857143*exp(0.0148588410104012*Na_o) - 0.142857142857143
f_NaK = 1.0/(0.0365*sigma*exp(-VFonRT) + 0.1245*exp(-0.1*VFonRT) + 1.0)
I_NaK = 1.0*I_NaKMax*K_o*f_NaK/((K_mKo + K_o)*((K_mNai/Na_i)**1.5 + 1.0))

component("Na background current I_bNa")
I_bNa = G_bNaMax*(-E_Na + V)

dm_dt = Conditional(Ge(V, -90.0), a_m*(-m + 1.0) - b_m*m, 0.0)
dh_dt = a_h*(-h + 1.0) - b_h*h
dj_dt = a_j*(-j + 1.0) - b_j*j